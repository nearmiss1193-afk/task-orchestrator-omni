    <!-- SCRIPTS -->
    <script>
        let funnelData = { problem:'', teamSize:'', season:'', action:'' };
        function openFunnel() { document.getElementById('funnelOverlay').classList.add('active'); document.body.style.overflow = 'hidden'; }
        function closeFunnel() { document.getElementById('funnelOverlay').classList.remove('active'); document.body.style.overflow = ''; setTimeout(() => { hideAllSteps(); document.getElementById('funnelStep1').classList.add('active'); }, 300); }
        function hideAllSteps() { document.querySelectorAll('.funnel-step').forEach(step => step.classList.remove('active')); }
        function goToStep(n, sel) {
            if(n===2 && sel) funnelData.problem = sel;
            if(n===3 && sel) funnelData.teamSize = sel;
            if(n===4 && sel) funnelData.season = sel;
            hideAllSteps(); document.getElementById('funnelStep'+n).classList.add('active');
        }
        function showContactForm() { funnelData.action='free_trial'; hideAllSteps(); document.getElementById('funnelStepContact').classList.add('active'); }
        function showBookingOption() { funnelData.action='talk_first'; hideAllSteps(); document.getElementById('funnelStepBooking').classList.add('active'); }
        function showPaymentForm() { funnelData.action='sign_up_now'; hideAllSteps(); document.getElementById('funnelStepPayment').classList.add('active'); }
        function handleOtherInput(input) { document.getElementById('otherPainSubmit').classList.toggle('visible', input.value.trim().length > 0); }
        function submitOtherPain() {
            const val = document.getElementById('otherPainInput').value.trim();
            if(val) { funnelData.customProblem = val; funnelData.problem = 'other'; closeFunnel(); setTimeout(() => alert('Thanks! Our team will reach out about: "'+val+'"'), 300); }
        }
        document.getElementById('funnelOverlay').addEventListener('click', function(e) { if(e.target === this) closeFunnel(); });
        let isAnnual = false;
        function toggleBilling() {
            isAnnual = !isAnnual;
            document.getElementById('billingToggle').classList.toggle('active', isAnnual);
            document.getElementById('monthlyLabel').classList.toggle('active', !isAnnual);
            document.getElementById('annualLabel').classList.toggle('active', isAnnual);
            document.querySelectorAll('.pricing-amount').forEach(el => {
                const m = el.dataset.monthly; const a = el.dataset.annual;
                if(m && a) el.textContent = isAnnual ? a : m;
            });
        }
    </script>

