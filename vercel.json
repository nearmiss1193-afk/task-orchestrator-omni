{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/vapi/webhook",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/apps/portal/public/landing/$1"
        }
    ]
}